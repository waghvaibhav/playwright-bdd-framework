  "scripts": {
    "test": "cucumber-js",
    "test:smoke:qa": "cross-env ENV=qa cucumber-js --tags \"@smoke\"",
    "test:smoke:stage": "cross-env ENV=stage cucumber-js --tags \"@smoke\"",
    "test:smoke:prod": "cross-env ENV=prod cucumber-js --tags \"@smoke\"",
    "test:regression:qa": "cross-env ENV=qa cucumber-js --parallel 4 --tags \"@regression\"",
    "test:regression:stage": "cross-env ENV=stage cucumber-js --parallel 4 --tags \"@regression\"",
    "test:regression:prod": "cross-env ENV=prod cucumber-js --parallel 4 --tags \"@regression\"",
    "test:smoke+regression:qa": "cross-env ENV=qa cucumber-js --tags \"@smoke or @regression\"",
    "test:smoke+regression:stage": "cross-env ENV=stage cucumber-js --tags \"@smoke or @regression\"",
    "test:smoke+regression:prod": "cross-env ENV=prod cucumber-js --tags \"@smoke or @regression\"",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "test:smoke:qa:report": "npm run test:smoke:qa && npm run allure:generate && npm run allure:open"
  },
